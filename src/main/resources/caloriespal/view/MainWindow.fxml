<AnchorPane prefHeight="650.0" prefWidth="1200.0" stylesheets="@../../styles/mainwindow.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="caloriespal.view.MainWindowController">
